<h1 class="page-heading">Edit Videos</h1>
<div>
	<a href="/admin">Back</a>
	<a href="/admin">Admin Home</a>
	<table class="admin-table" cellspacing="10">
		<thead>
			<tr>
				<td>ID</td>
				<td>Title</td>
				<td>URL ID</td>
				<td>Preview</td>
				<td>Actions</td>
			</tr>
		</thead>
		<tbody>
		<% if (print) { %>
			<% for(var i = 0; i < print.length; i++) { %>
			<tr class="admin-table__row">
				<td><%= print[i].video_id%></td>
				<td><%= print[i].title%></td>
				<td><%= print[i].url_video_id%></td>
				<td><%= print[i].gif%></td>
				<td>
					<a href="/edit_videos/<%= print[i].video_id %>" class="">Edit</a>
					<a href="/delete_video/<%= print[i].video_id %>" class="">Delete</a>
				</td>
			</tr>
			<% } %>
		<% } %>
		</tbody>
	</table>
	<div>Add entry:</div>
	<div class="admin-form">
		<form action="/add_video" method="POST">
			<input class="admin-input" type="text" name="title" placeholder="Title"><br>
			<input class="admin-input" type="text" name="url_video_id" placeholder="Video URL ID"><br>
			<input class="admin-input" type="text" name="gif" placeholder="Gif"><br>
			<button class="admin-button" type="submit">Save</button>
		</form>
	</div>
</div>
